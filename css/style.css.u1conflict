		filter: progid:DXImageTransform.Microsoft.gradient(startColorStr='#353535', EndColorStr='#252525'); 
}
#footer-container footer:after {
	content:"";
	height: 360px;
	width: 100%;
	position: absolute;
	display: block;
	top: -360px;
	z-index: 0;
	background: url("../images/image11293.png") no-repeat right bottom;
}
#footer-container footer{
	position: relative;
	color:white;
}
#footer-container > footer > div {
