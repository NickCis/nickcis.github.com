html, body {height: 100%;}
body{
	font-family: Georgia, "Times New Roman", Times, serif;
	background-color: #777777;
	background-image: -webkit-gradient(linear, right top, left top, color-stop(0.5, transparent), color-stop(0.5, rgba(255,255,255, 0.2)));
	background-image: -webkit-linear-gradient(right center, transparent 50%, rgba(255,255,255, 0.2) 50%); /* Chrome 10+, Saf5.1+ */
	background-image:	-moz-linear-gradient(right center, transparent 50%, rgba(255,255,255, 0.2) 50%); /* FF 3.6 */
	background-image:	 -ms-linear-gradient(right center, transparent 50%, rgba(255,255,255, 0.2) 50%); /* IE10 */
	background-image:	  -o-linear-gradient(right center, transparent 50%, rgba(255,255,255, 0.2) 50%); /* Opera 11.10+ */
	background-image:		 linear-gradient(right center, transparent 50%, rgba(255,255,255, 0.2) 50%);
	-webkit-background-size: 50px; /* Saf3-4 */
		   -moz-background-size: 50px; /* FF3.6 */
				background-size: 50px; /* Opera, IE9, Saf5, Chrome, FF4 */
}
h2{
	font-size: 55px;
}

h3{
	font-size: 40px;
}
.wrapper{
	margin:0 auto 0 auto;
	width:960px;
}
.blue {
	color: #0390c5;
}

#header-container{
	width: 100%;
	height:85px;
	position: absolute;
	top: 0;
	left: 0;
	z-index: 100;
	background-color:#3e3e3e;
	-webkit-box-shadow: 0 0 20px black;
	   -moz-box-shadow: 0 0 20px black;
			box-shadow: 0 0 20px black;
}

#title {
	font-family:Arial, Helvetica, sans-serif;
	font-weight: bold;
	float:left;
	font-size: 80px;
	color:white;
	line-height: 80px;
	text-shadow: grey 1px 1px 0, grey 2px 2px 0, grey 3px 3px 0, grey 4px 4px 0;
}
#title > .blue {
	text-shadow: #024157 1px 1px 0,#024157 2px 2px 0,#024157 3px 3px 0, #024157 4px 4px 0;
}
.subtitle {
	font-family:Arial, Helvetica, sans-serif;
	font-weight: normal;
	font-size: 40px;
	color: white;
	line-height: 30px;
}
.subtitle.first {
	padding-top: 5px;
}

aside{
	text-align: center;
	position: fixed;
	left: 0;
	top:100px;
	color:white;
	padding:20px;
	background-color:#0e4052;
	-webkit-border-top-right-radius: 30px;
	-webkit-border-bottom-right-radius: 30px;
	   -moz-border-radius-topright: 30px;
	   -moz-border-radius-bottomright: 30px;
			border-top-right-radius: 30px;
			border-bottom-right-radius: 30px;
	-webkit-box-shadow:5px 5px 2px #0b303e;
	   -moz-box-shadow:5px 5px 2px #0b303e;
			box-shadow:5px 5px 2px #0b303e;
	-moz-background-clip: padding; -webkit-background-clip: padding-box; background-clip: padding-box;
}
aside ul {
	list-style-type: none;
	margin: 0 -10px -5px -10px;
}
aside ul li {
	margin-top: 5px;
}
aside ul li:first-child {
	margin-top:0px;
}

#main {
	min-height: 100%;
	height: auto !important;
	height: 100%;
	margin-bottom: -180px;
	background-color: #dcdcdc;
	-webkit-box-shadow: 0 0 20px black;
	   -moz-box-shadow: 0 0 20px black;
			box-shadow: 0 0 20px black;
}
#main > div {
	position: relative;
	z-index: 1;
	padding-top: 85px;
	padding-bottom: 180px;
}
/* ===========================*/
#main article{
	width:900px;
	text-shadow:none;
	margin: auto;
}

#main header h2{
	font-family: 'Terminal Dosis Light', cursive, sans-serif;
	text-align: right;
	padding-bottom:30px;
	text-shadow: #0390c5 1px 1px 0,#0390c5 2px 2px 0,#0390c5 3px 3px 0, #0390c5 4px 4px 0;
}
#main header h3{
	color: black;	
}
#main header h3 > span {
	font-family:Arial, Helvetica, sans-serif;
	font-weight: bold;
}
#main h4 {
	font-size: 16px;
	font-weight: bold;
	font-style: italic;
	color: black;
	padding: 20px 0 10px 0;
}

#main p {
	font-size: 14px;
